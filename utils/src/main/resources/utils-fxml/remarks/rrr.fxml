<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.text.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.text.Font?>

<AnchorPane maxHeight="800.0" prefHeight="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ru.wert.datapik.utils.remarks.Remark_ACCController">
    <children>
        <StackPane layoutX="5.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
            <children>
                <VBox fx:id="vbMain" layoutX="5.0" maxHeight="1.7976931348623157E308" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
                    <children>
                        <ScrollPane fitToHeight="true" fitToWidth="true" hbarPolicy="NEVER" maxHeight="1.7976931348623157E308" prefViewportHeight="40.0" prefViewportWidth="600.0" VBox.vgrow="ALWAYS">
                            <content>
                                <StackPane>
                                    <children>
                                        <StackPane fx:id="spWaitingBlind">
                                            <children>
                                                <HBox alignment="CENTER" spacing="10.0" styleClass="-fx-background-color: grey">
                                                    <children>
                                                        <ProgressIndicator maxHeight="50.0" maxWidth="50.0" StackPane.alignment="CENTER">
                                                            <StackPane.margin>
                                                                <Insets />
                                                            </StackPane.margin>
                                                        </ProgressIndicator>
                                                        <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Загрузка...">
                                                            <font>
                                                                <Font name="System Italic" size="24.0" />
                                                            </font>
                                                        </Text>
                                                    </children>
                                                </HBox>
                                            </children>
                                        </StackPane>
                                        <VBox maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" spacing="5.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
                                            <children>
                                                <TextArea fx:id="taRemarksText" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" prefRowCount="1" VBox.vgrow="NEVER">
                                                    <VBox.margin>
                                                        <Insets />
                                                    </VBox.margin>
                                                </TextArea>
                                                <StackPane VBox.vgrow="ALWAYS">
                                                    <children>
                                                        <Button fx:id="btnAddPhoto" mnemonicParsing="false" onAction="#addPhoto" text="+ Фото" StackPane.alignment="TOP_RIGHT">
                                                            <font>
                                                                <Font size="18.0" />
                                                            </font>
                                                            <HBox.margin>
                                                                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                                                            </HBox.margin>
                                                            <StackPane.margin>
                                                                <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                                                            </StackPane.margin>
                                                        </Button>
                                                        <VBox fx:id="vbRemarksEntryContainer" spacing="5.0" StackPane.alignment="TOP_LEFT" />
                                                    </children>
                                                </StackPane>
                                            </children>
                                        </VBox>
                                    </children>
                                </StackPane>
                            </content>
                            <VBox.margin>
                                <Insets top="10.0" />
                            </VBox.margin>
                        </ScrollPane>
                        <VBox VBox.vgrow="NEVER">
                            <children>
                                <Separator prefWidth="200.0">
                                    <VBox.margin>
                                        <Insets bottom="10.0" top="10.0" />
                                    </VBox.margin>
                                </Separator>
                                <HBox alignment="CENTER_RIGHT" spacing="10.0">
                                    <children>
                                        <StackPane fx:id="spIndicator" maxHeight="39.0" maxWidth="39.0" minHeight="39.0" minWidth="39.0" prefHeight="39.0" prefWidth="39.0" />
                                        <Button fx:id="btnCancel" cancelButton="true" mnemonicParsing="false" onAction="#cancel" prefHeight="39.0" prefWidth="126.0" text="ОТМЕНА">
                                            <font>
                                                <Font name="System Bold" size="18.0" />
                                            </font>
                                        </Button>
                                        <Button fx:id="btnOk" defaultButton="true" mnemonicParsing="false" onAction="#ok" prefHeight="39.0" prefWidth="132.0" text="ОК">
                                            <font>
                                                <Font name="System Bold" size="18.0" />
                                            </font>
                                        </Button>
                                    </children>
                                    <opaqueInsets>
                                        <Insets />
                                    </opaqueInsets>
                                    <padding>
                                        <Insets right="10.0" />
                                    </padding>
                                </HBox>
                            </children>
                        </VBox>
                    </children>
                </VBox>
            </children>
        </StackPane>
    </children>
    <padding>
        <Insets bottom="5.0" left="5.0" right="5.0" />
    </padding>
</AnchorPane>

