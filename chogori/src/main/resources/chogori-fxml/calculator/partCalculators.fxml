<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.image.*?>
<?import javafx.scene.text.*?>
<?import javafx.geometry.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox fx:id="calculator" prefHeight="500.0" prefWidth="500.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ru.wert.datapik.chogori.calculator.part_calculator.PartCalculatorController">
   <children>
      <HBox alignment="CENTER_LEFT" spacing="10.0">
         <children>
            <Label text="Материал:">
               <font>
                  <Font size="14.0" />
               </font>
            </Label>
            <ComboBox fx:id="cmbxMaterial" maxWidth="1.7976931348623157E308" minWidth="-Infinity" HBox.hgrow="ALWAYS" />
            <HBox alignment="CENTER_RIGHT" prefHeight="32.0" prefWidth="0.0">
               <children>
                  <ImageView fx:id="ivErase" fitHeight="32.0" fitWidth="32.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@../../chogori-pics/btns/erase.png" />
                     </image>
                  </ImageView>
               </children>
            </HBox>
         </children>
         <VBox.margin>
            <Insets bottom="5.0" />
         </VBox.margin>
      </HBox>
      <VBox alignment="CENTER_LEFT" prefWidth="100.0" spacing="5.0" />
      <HBox alignment="CENTER_LEFT" spacing="10.0">
         <children>
            <Label text="Основные параметры">
               <font>
                  <Font name="System Bold" size="16.0" />
               </font>
            </Label>
            <ImageView fx:id="ivHelpOnPartParameters" fitHeight="16.0" fitWidth="16.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../../chogori-pics/btns/help.png" />
               </image>
            </ImageView>
         </children>
      </HBox>
      <HBox alignment="CENTER_LEFT" fillHeight="false" maxHeight="-Infinity" minWidth="-Infinity" prefHeight="50.0" spacing="5.0" VBox.vgrow="NEVER">
         <children>
            <VBox alignment="CENTER_LEFT" prefWidth="100.0" spacing="5.0">
               <children>
                  <Label text="A, мм">
                     <font>
                        <Font size="14.0" />
                     </font></Label>
                  <TextField fx:id="tfA" maxWidth="-Infinity" minWidth="-Infinity" prefWidth="115.0" />
               </children>
            </VBox>
            <VBox alignment="CENTER_LEFT" prefWidth="100.0" spacing="5.0">
               <children>
                  <Label text="B, мм">
                     <font>
                        <Font size="14.0" />
                     </font></Label>
                  <TextField fx:id="tfB" maxWidth="-Infinity" minWidth="-Infinity" prefWidth="115.0" />
               </children>
            </VBox>
            <Separator orientation="VERTICAL" prefHeight="200.0" />
            <VBox alignment="CENTER_LEFT" prefWidth="100.0" spacing="5.0">
               <children>
                  <HBox alignment="CENTER_LEFT" spacing="10.0">
                     <children>
                        <Label text="Масса, кг">
                           <font>
                              <Font size="14.0" />
                           </font>
                        </Label>
                        <ImageView fx:id="ivHelpOnWeight" fitHeight="16.0" fitWidth="16.0" pickOnBounds="true" preserveRatio="true">
                           <image>
                              <Image url="@../../chogori-pics/btns/help.png" />
                           </image>
                        </ImageView>
                     </children>
                  </HBox>
                  <TextField fx:id="tfWeight" editable="false" maxWidth="-Infinity" minWidth="-Infinity" prefWidth="115.0" />
               </children>
            </VBox>
            <VBox alignment="CENTER_LEFT" prefWidth="100.0" spacing="5.0">
               <children>
                  <Label text="Покрытие, м.кв">
                     <font>
                        <Font size="14.0" />
                     </font>
                  </Label>
                  <TextField fx:id="tfCoat" editable="false" maxWidth="-Infinity" minWidth="-Infinity" prefWidth="115.0" />
               </children>
            </VBox>
         </children>
      </HBox>
      <Separator maxWidth="1.7976931348623157E308">
         <VBox.margin>
            <Insets top="15.0" />
         </VBox.margin>
      </Separator>
      <HBox alignment="CENTER_LEFT" spacing="10.0">
         <children>
            <Label text="Технологические операции">
               <font>
                  <Font name="System Bold" size="16.0" />
               </font>
            </Label>
            <ImageView fx:id="ivHelpOnTechnologicalProcessing" fitHeight="16.0" fitWidth="16.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../../chogori-pics/btns/help.png" />
               </image>
            </ImageView>
            <HBox alignment="CENTER_RIGHT" spacing="5.0" HBox.hgrow="ALWAYS">
               <children>
                  <Label text="Ед.изм." />
                  <ComboBox fx:id="cmbxTimeMeasurement" prefWidth="70.0" />
               </children>
            </HBox>
         </children>
         <VBox.margin>
            <Insets top="10.0" />
         </VBox.margin>
      </HBox>
      <ScrollPane fitToWidth="true" hbarPolicy="NEVER" maxWidth="1.7976931348623157E308" VBox.vgrow="ALWAYS">
         <content>
            <VBox maxWidth="1.7976931348623157E308" spacing="10.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
               <children>
                  <VBox maxWidth="1.7976931348623157E308">
                     <children>
                        <CheckBox fx:id="chbxCutting" mnemonicParsing="false" text="Резка">
                           <font>
                              <Font name="System Bold" size="14.0" />
                           </font></CheckBox>
                        <StackPane fx:id="spCuttingContainer" maxWidth="1.7976931348623157E308" />
                     </children>
                  </VBox>
                  <VBox maxWidth="1.7976931348623157E308">
                     <children>
                        <CheckBox fx:id="chbxBending" mnemonicParsing="false" text="Гибка">
                           <font>
                              <Font name="System Bold" size="14.0" />
                           </font></CheckBox>
                        <StackPane fx:id="spBendingContainer" maxWidth="1.7976931348623157E308" />
                     </children>
                  </VBox>
                  <VBox maxWidth="1.7976931348623157E308">
                     <children>
                        <CheckBox fx:id="chboxPainting" mnemonicParsing="false" text="Покраска">
                           <font>
                              <Font name="System Bold" size="14.0" />
                           </font></CheckBox>
                        <StackPane fx:id="spPaintingContainer" maxWidth="1.7976931348623157E308" />
                     </children>
                  </VBox>
               </children>
            </VBox>
         </content>
         <VBox.margin>
            <Insets left="10.0" top="10.0" />
         </VBox.margin>
      </ScrollPane>
   </children>
   <padding>
      <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
   </padding>
</VBox>
