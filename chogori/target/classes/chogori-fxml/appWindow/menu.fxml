<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.control.SeparatorMenuItem?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane maxHeight="-Infinity" minHeight="-Infinity" minWidth="-Infinity" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ru.wert.datapik.chogori.application.app_window.MenuController">
   <children>
      <VBox alignment="CENTER_LEFT" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0">
         <children>
            <HBox fx:id="menuBox" alignment="CENTER_LEFT">
               <children>
                  <MenuBar fx:id="menuBar" prefHeight="25.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0">
                    <menus>
                      <Menu mnemonicParsing="false" text="Файл">
                        <items>
                              <SeparatorMenuItem mnemonicParsing="false" />
                              <MenuItem fx:id="mQuit" mnemonicParsing="false" onAction="#quit" text="Выход" />
                        </items>
                      </Menu>
                        <Menu mnemonicParsing="false" text="Редактор">
                           <items>
                              <MenuItem fx:id="mOpenExcelFile" mnemonicParsing="false" onAction="#openExcelFile" text="Открыть файл Excel" />
                           </items>
                        </Menu>
                        <Menu mnemonicParsing="false" text="Номенклатура">
                           <items>
                              <MenuItem fx:id="mDetails" mnemonicParsing="false" onAction="#openDetails" text="Детали" />
                              <MenuItem fx:id="mAssembles" mnemonicParsing="false" onAction="#openAssembles" text="Сборки" />
                              <SeparatorMenuItem mnemonicParsing="false" />
                              <MenuItem fx:id="mDrafts" mnemonicParsing="false" onAction="#openDrafts" text="Чертежи" />
                              <MenuItem fx:id="mPassports" mnemonicParsing="false" onAction="#openPassports" text="Картотека" />
                           </items>
                        </Menu>
                        <Menu mnemonicParsing="false" text="Каталоги">
                           <items>
                              <MenuItem fx:id="mCatalogOfProducts" mnemonicParsing="false" onAction="#openCatalogOfProducts" text="Каталог изделий" />
                              <MenuItem fx:id="mCatalogOfFolders" mnemonicParsing="false" onAction="#openCatalogOfFolders" text="Каталог пакетов" />
                              <MenuItem fx:id="mCatalogOfFolders1" mnemonicParsing="false" onAction="#openCatalogOfMaterials" text="Каталог материалов" />
                           </items>
                        </Menu>
                        <Menu mnemonicParsing="false" text="Админ">
                           <items>
                              <MenuItem fx:id="mUsers" mnemonicParsing="false" onAction="#openUsers" text="Пользователи" />
                           </items>
                        </Menu>
                    </menus>
                  </MenuBar>
                  <Pane maxWidth="1.7976931348623157E308" minWidth="100.0" HBox.hgrow="ALWAYS" />
               </children>
               <padding>
                  <Insets right="5.0" />
               </padding>
            </HBox>
         </children>
      </VBox>
   </children>
</AnchorPane>
